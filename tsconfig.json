{
    "extends": "./tsconfig.base.json",
    "include": [
        "./packages/trackback-vc/src/**/*.ts",
        "./packages/trackback-did/src/**/*.ts",
        "./packages/trackback-agent/src/**/*.ts",
        "./packages/trackback-key/src/**/*.ts",
    ],
    "compilerOptions": {
        "paths": {
            "@trackback/vc": [
                "./packages/trackback-vc/src"
            ],
            "@trackback/did": [
                "./packages/trackback-did/src"
            ],
            "@trackback/agent": [
                "./packages/trackback-agent/src"
            ],
            "@trackback/key": [
                "./packages/trackback-key/src"
            ],
        }
    },
    "typedocOptions": {
        "entryPoints": [
            "packages/trackback-vc/src/index.ts",
            "packages/trackback-did/src/index.ts",
            "packages/trackback-agent/src/index.ts",
            "packages/trackback-key/src/index.ts",
        ],
        "out": "docs"
    }
}